{
  "name": "vue-mapbox-map",
  "version": "0.8.17",
  "description": "A Vue component for dynamic Mapbox GL JS maps!",
  "homepage": "https://github.com/cityseer/vue-mapbox-map",
  "bugs": "https://github.com/cityseer/vue-mapbox-map/issues",
  "license": "GPL-3.0",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "bundle": "vue-cli-service build --target lib --name vue-mapbox-map src/main.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-geocoder": "^3.1.4",
    "mapbox-gl": "^0.53.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "electron": "^4.0.8",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.4",
    "vue": "^2.6.6",
    "vue-template-compiler": "^2.5.21",
    "vuepress": "^1.0.0-alpha.44"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
